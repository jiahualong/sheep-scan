{
  "name": "图片检测",
  "description": "检测页面中的图片",
  "version": "0.1",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "browser_action": {
    "default_title": "检测图片",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/getimg.js"]
    }
    ],
  "manifest_version": 2
}
